{
  "name": "themenu",
  "version": "0.0.1",
  "description": "The Menu Online is a service for restaurants and small businesses to create online version of their menus or pricelists",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && webpack -p --env.production",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "start": "webpack-dev-server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,json,css,md}": [
        "flow focus-check",
        "eslint --fix",
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*.min.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/komlew/themenu.git"
  },
  "keywords": [
    "menu",
    "themenu",
    "themenuonline",
    "menuonline",
    "onlinemenus",
    "pricelist"
  ],
  "author": "Alex Komlev",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/komlew/themenu/issues"
  },
  "homepage": "https://komlew.github.io/themenu/",
  "dependencies": {
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint": "5.15.2",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^1.5.1",
    "flow-bin": "0.95.1",
    "flow-typed": "2.5.1",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-template": "6.2.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  }
}
